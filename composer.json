{
    "name": "check-fraud-customer/check-fraud-customer",
    "description": "Laravel package for checking customer fraud based on courier delivery history.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "fraud",
        "checker",
        "courier",
        "delivery",
        "customer"
    ],
    "autoload": {
        "psr-4": {
            "CheckFraudCustomer\\CourierFraudChecker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CheckFraudCustomer\\CourierFraudChecker\\CheckFraudCustomerServiceProvider"
            ],
            "aliases": {
                "CheckFraudCustomer": "CheckFraudCustomer\\CourierFraudChecker\\Facade\\CheckFraudCustomer"
            }
        }
    },
    "authors": [
        {
            "name": "Ahad",
            "email": "ahaduzzamanaapon@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0",
        "illuminate/http": "^10.0 || ^11.0 || ^12.0"
    }
}